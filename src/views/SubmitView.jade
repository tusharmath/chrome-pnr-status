form(role="form")
	.form-group
		input.text-center.form-control(type='text', placeholder='Enter your PNR number', ng-model="SubmitView.pnrNumber", ng-disabled="isWorking")
	.form-group
		button.btn.form-control(type='button', ng-click="SubmitView.formSubmit()", class="{{SubmitView.button_class}}") {{SubmitView.button_value}}
